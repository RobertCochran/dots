tls_starttls on
tls_fingerprint 22:A5:7D:EF:04:B0:F9:AF:6F:A3:DB:6C:D5:D3:4C:01:04:9F:E1:62

from alex@strugee.net
auth on
user alex@strugee.net

# steevie is the default account
account default : steevie

logfile ~/.msmtp.log
