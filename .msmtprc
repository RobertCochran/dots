# msmtp configuration
# Alex Jordan
# 24 March 2015

defaults

port 587

tls on

# steevie
account steevie

host smtp.strugee.net
tls_certcheck on
tls_starttls on
tls_fingerprint 1A:BD:0D:F5:E2:6D:9A:7D:D2:D7:5F:CF:F9:D2:3F:A5:C7:F4:10:BF

from alex@strugee.net
auth on
user alex@strugee.net

# steevie is the default account
account default : steevie

logfile ~/.msmtp.log
