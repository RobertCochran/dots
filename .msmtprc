tls_starttls on
tls_fingerprint 57:0C:65:FD:34:60:FB:75:CF:A1:AD:61:A6:98:43:2B:45:EF:40:20

from alex@strugee.net
auth on
user alex@strugee.net

# steevie is the default account
account default : steevie

logfile ~/.msmtp.log
